{
    "name": "kindleit direct",
    "description": "Post webpage to kindle directly by using kindleit",
    "id": "ljjpaogbkhfbmbcamjdchjjfpcopjafm",
    "manifest_version": 2,
    "minimum_chrome_version": "35",
    "version": "1.0.1",
    "permissions": [
        "http://fivefilters.org/kindle-it",
        "https://fivefilters.org/kindle-it",
        "activeTab",
        "notifications",
        "contextMenus"
    ],
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "browser_action": {
        "default_icon": "icon.png"
     },
    "options_page": "options.html",
    "background": {
        "scripts": ["jquery-2.0.2.min.js", "background.js"]
    }
}
